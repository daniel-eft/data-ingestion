{
    "name": "ingestro-case-study",
    "version": "1.0.0",
    "description": "Production-grade data ingestion pipeline",
    "main": "dist/api/server.js",
    "scripts": {
        "start:api": "node dist/src/api/server.js",
        "start:worker": "node dist/src/worker/worker.js",
        "dev:api": "ts-node-dev --respawn --transpile-only src/api/server.ts",
        "dev:worker": "ts-node-dev --respawn --transpile-only src/worker/worker.ts",
        "build": "tsc",
        "test": "jest",
        "test:unit": "jest tests/unit",
        "test:integration": "jest tests/integration",
        "test:e2e": "jest tests/e2e",
        "migrate": "node dist/scripts/run-migrations.js",
        "seed": "node dist/scripts/seed-database.js",
        "lint": "eslint src/**/*.ts"
    },
    "dependencies": {
        "bullmq": "^4.12.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "csv-parser": "^3.0.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "ioredis": "^5.3.2",
        "JSONStream": "^1.3.5",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.11.3",
        "winston": "^3.10.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@types/compression": "^1.7.3",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.5",
        "@types/JSONStream": "npm:@types/jsonstream@^0.8.33",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.6.2",
        "@types/pg": "^8.10.2",
        "@types/supertest": "^2.0.12",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2"
    }
}